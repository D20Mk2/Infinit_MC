    // 配件的显示名称，支持使用语言文件定义
    "name": "xiaomantou.attachment.ax50_muzzle.name",
    // 配件效果文件，对应 attachments/display/xxx_display.json，其中前缀"attachments/display/"和后缀".json"不需要包含
    "display": "xiaomantou:ax50_muzzle_display",
    // 配件参数文件，对应 attachments/data/xxx_data.json，其中前缀"attachments/data/"和后缀".json"不需要包含
    "data": "xiaomantou:ax50_muzzle_data",
    // 创造栏标签页类别，因为创建的是枪口配件，所以类型选择 muzzle
    "type": "muzzle"
  }